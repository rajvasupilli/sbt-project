{
  "family": "sbt-task-definition",
  "executionRoleArn": "arn:aws:iam::597993487797:role/ecsTaskExecutionRole",
  "containerDefinitions": [
      {
          "name": "sbt-container",
          "image": "597993487797.dkr.ecr.us-east-1.amazonaws.com/sbtrepo:latest",
          "cpu": 512,
          "portMappings": [
            {
              "hostPort": 9000,
              "protocol": "tcp",
              "containerPort": 9000
            }
          ],
          "memory": 1024,
          "essential": true,
          "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
              "awslogs-group": "/ecs/sbt-project",
              "awslogs-region": "us-east-1",
              "awslogs-stream-prefix": "ecs"
            }
          }
      }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "2048",
  "memory": "4096"
}
