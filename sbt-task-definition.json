{
    "family": "sbt-task-definition",
    "executionRoleArn": "arn:aws:iam::009089246773:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "sbt-container",
            "image": "009089246773.dkr.ecr.us-east-1.amazonaws.com/sbt-ecr",
            "portMappings": [
                {
                    "containerPort": 9000,
                    "hostPort": 9000,
                    "protocol": "tcp"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "awslogs-sbtservice",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "awslogs-sbt"
                }
            },
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}
